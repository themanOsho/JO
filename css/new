            <div class="w3-padding w3-mobile w3-bar-item" data-aos="zoom-in-up" data-aos-duration="5000">
                <textarea name="Subject"
                    class="w3-input w3-padding-large w3-bottombar w3-border-cyan w3-round-xlarge w3-card-4"
                    placeholder="Subject" required></textarea>
            </div>
            <div class="w3-padding w3-bar-item">
                <button type="submit" data-aos="zoom-in-up" data-aos-duration="5000"
                    class="w3-btn w3-padding-large w3-round-xlarge w3-bottombar w3-border-cyan w3-text-gray w3-card-4"
                    style="margin-left: auto;">Send</button>
                <span id="welmsg" class="w3-margin"></span>
            </div>
        </form>
        <script>
            const scriptURL = 'https://script.google.com/macros/s/AKfycbyjcyzV1yJedWFTUnz8RFkMEZG67M0XOxCEwQf9hYiq9KT3Gptk04iJuMvBQvxvZ_MuKg/exec'
            const form = document.forms['submit-to-google-sheet']
            const welmsg = document.getElementById('welmsg')

            form.addEventListener('submit', e => {
                e.preventDefault()
                fetch(scriptURL, { method: 'POST', body: new FormData(form) })
                    .then(response => {
                        welmsg.innerHTML = "Sent!"
                        setTimeout(function () {
                            welmsg.innerHTML = ""
                        }, 5000)
                        form.reset()
                    })
                    .catch(error => console.error('Error!', error.message))
            })
        </script>
